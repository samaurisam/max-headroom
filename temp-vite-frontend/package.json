{
  "name": "temp-vite-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "s3-sync": "node scripts/deploy-s3.mjs",
    "invalidate": "node -e \"require('dotenv').config(); const d = process.env.DISTRIBUTION_ID; if (!d) { console.log('Skipping CloudFront invalidation'); process.exit(0); } console.log('Invalidating:', d); require('child_process').execSync(`aws cloudfront create-invalidation --distribution-id ${d} --paths \"/*\"`, { stdio: 'inherit' });\"",
    "deploy": "npm run build && npm run s3-sync && npm run invalidate"
  },
  "dependencies": {
    "@elevenlabs/client": "^0.8.0",
    "@elevenlabs/react": "^0.8.1",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@react-three/postprocessing": "^3.0.4",
    "@vfx-js/core": "^0.8.0",
    "elevenlabs": "^1.59.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4",
    "socket.io-client": "^4.8.1",
    "three": "^0.180.0",
    "wavesurfer.js": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@gltf-transform/cli": "^4.2.1",
    "@gltf-transform/core": "^4.2.1",
    "@gltf-transform/functions": "^4.2.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  }
}
